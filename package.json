{
  "name": "react-challenge-master",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "typecheck": "tsc --noEmit",
    "build:css": "postcss src/styles.css --output src/generated/tw.css",
    "build:components": "esbuild src/custom-elements/index.ts --bundle --format=iife --outfile=dist/my-form.js --minify --jsx=automatic --loader:.css=text",
    "build": "pnpm build:css && pnpm build:components && pnpm typecheck",
    "watch:tailwind": "postcss src/styles.css --output src/generated/tw.css --watch",
    "watch:components": "esbuild src/custom-elements/index.ts --bundle --format=iife --outfile=dist/my-form.js --jsx=automatic --watch --servedir=. --loader:.css=text",
    "dev": "concurrently -n CSS,JS \"pnpm watch:tailwind\" \"pnpm watch:components\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.17.1",
  "dependencies": {
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.13",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.1",
    "esbuild": "^0.25.10",
    "postcss-cli": "^11.0.1",
    "tailwindcss": "^4.0.0",
    "typescript": "^5.9.2"
  }
}
